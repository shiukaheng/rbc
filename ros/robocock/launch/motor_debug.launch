<?xml version="1.0"?>
<launch>
    <node pkg="rosserial_arduino" type="serial_node.py" name="rosserial_node" output="screen">
        <param name="port" value="/dev/ttyACM0" />
    </node>
    <node pkg="rqt_gui" type="rqt_gui" name="rqt_gui" output="screen" respawn="false" args="--perspective-file $(find robocock)/config/wheel_debug.perspective" launch-prefix="bash -c 'sleep 3; $0 $@'"/>
</launch>